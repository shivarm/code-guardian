{
  "rules": [
    {
      "name": "AWS Access Key ID",
      "pattern": "AKIA[0-9A-Z]{16}",
      "flags": "g"
    },
    {
      "name": "AWS Secret Access Key",
      "pattern": "(?:aws.{0,20})?(?:secret|secret_access_key|secretaccesskey|secretKey)\\s*[=:\\s']\\s*[0-9A-Za-z/+=]{40}",
      "flags": "gi"
    },
    {
      "name": "JWT",
      "pattern": "eyJ[A-Za-z0-9\\-_]+\\.[A-Za-z0-9\\-_]+\\.[A-Za-z0-9\\-_]+",
      "flags": "g"
    },
    {
      "name": "Slack token",
      "pattern": "xox[baprs]-[0-9a-zA-Z]{10,}",
      "flags": "g"
    },
    {
      "name": "Private RSA key",
      "pattern": "-----BEGIN\\s+PRIVATE\\s+KEY-----",
      "flags": "g"
    },
    {
      "name": "Heroku API key",
      "pattern": "heroku\\s*[:=]\\s*[0-9a-fA-F]{32}",
      "flags": "gi"
    },
    {
      "name": "Azure Storage Account Key",
      "pattern": "(AccountKey\\s*[:=]\\s*)[A-Za-z0-9+/]{88}==",
      "flags": "gi"
    },
    {
      "name": "Google Cloud API Key",
      "pattern": "AIza[0-9A-Za-z\\-_]{35}",
      "flags": "g"
    }
  ]
}
